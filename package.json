  "author": "Matthew D. Mower <mdmower@cmphys.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mdmower/doi-resolver/issues"
  },
  "homepage": "https://github.com/mdmower/doi-resolver#readme",
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "bootstrap": "^5.3.3",
    "citeproc": "^2.4.63"
  },
  "devDependencies": {
