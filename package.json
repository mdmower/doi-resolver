{
  "name": "doi-resolver-chrome",
  "version": "5.0.0",
  "description": "An extension for Chrome to quickly resolve digital object identifiers to their web destinations",
  "private": true,
  "scripts": {
    "lint": "eslint src build",
    "format": "prettier --write \"build/**/*.js\" \"src/**/*.ts\" \"html/**/*.html\" \"src/**/*.scss\"",
    "build": "node build/dist.js",
    "build-debug": "node build/dist.js --debug",
    "update-csl": "node build/update-csl.js",
    "pkg": "node build/pkg.js",
    "clean": "node build/clean.js",
    "release": "npm run lint && npm run clean && npm run build && npm run pkg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/doi-resolver-chrome.git"
  },
  "keywords": [
    "doi",
    "resolve",
    "resolver",
    "chrome",
    "extension"
  ],
  "author": "Matthew D. Mower <mdmower@cmphys.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mdmower/doi-resolver-chrome/issues"
  },
  "homepage": "https://github.com/mdmower/doi-resolver-chrome#readme",
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "citeproc": "^2.4.62"
  },
  "devDependencies": {
    "@types/chrome": "0.0.197",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^18.8.3",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@xmldom/xmldom": "^0.8.2",
    "archiver": "^5.3.1",
    "clean-css": "^5.3.1",
    "colors": "^1.4.0",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "html-minifier": "^4.0.0",
    "minimist": "^1.2.6",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  }
}
